{
  "name": "create",
  "customName": false,
  "customRole": false,
  "handler": "create/handler.handler",
  "timeout": 6,
  "memorySize": 128,
  "custom": {
    "excludePatterns": [],
    "envVars": []
  },
  "endpoints": [
    {
      "path": "sites",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/x-www-form-urlencoded": {
          "body": "$input.json('$')",
          "stage": "$context.stage"
        }
      },
      "responses": {
        "default": {
          "statusCode": "302",
          "responseParameters": {
            "method.response.header.Location": "integration.response.body.location"
          }
        }
      }
    }
  ],
  "events": []
}